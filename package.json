        "clsx": "^2.0.0",
        "graphql": "^16.8.1",
        "next": "14.0.1",
        "react": "^18",
        "react-dom": "^18",
        "sass": "^1.69.5",
        "swiper": "^11.0.3"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "5.0.0",
        "@graphql-codegen/client-preset": "^4.1.0",
        "@next/env": "^14.0.3",
        "@types/lodash": "^4.14.200",
