    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "prettier": "prettier --write \"**/*.{ts,tsx,json,scss}\"",
        "eslint": "eslint --quiet \"src/**/*.{ts,tsx,json}\"",
        "lint-staged": "lint-staged",
        "prepare": "husky install",
