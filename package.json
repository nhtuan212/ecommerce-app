        "eslint": "^8",
        "eslint-config-next": "14.0.1",
        "eslint-plugin-json": "^3.1.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.0.2",
        "lodash": "^4.17.21",
        "postcss": "^8",
        "prettier": "^3.0.3",
        "tailwind-merge": "^2.0.0",
        "tailwindcss": "^3.3.0",
        "typescript": "^5"
